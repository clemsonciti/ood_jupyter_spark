# Job submission configuration file
#
---

batch_connect:
  template: "basic"

script:
  native:
    - "-N"
    - "j_spark"
    - "-q"
    - "<%= pbs_queue %>"
    - "-l"
    - "select=<%= pbs_select %>:ncpus=<%= pbs_ncpus %>:mpiprocs=<%= pbs_ncpus %>:mem=<%= pbs_mem %>:interconnect=<%= pbs_interconnect %>:ngpus=<%= pbs_ngpus %>:gpu_model=<%= pbs_gpu_model %>,walltime=<%= pbs_walltime %>"


